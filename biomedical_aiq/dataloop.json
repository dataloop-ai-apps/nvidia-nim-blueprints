{
  "name": "biomedical-aiq-research-agent",
  "displayName": "NVIDIA Biomedical AI-Q Research Agent",
  "description": "Biomedical research agent with Plan-Execute-Reflect loop, web search, RAG, and virtual screening. Adapted from NVIDIA Biomedical AI-Q Research Agent Blueprint.",
  "version": "0.0.36",
  "scope": "public",
  "codebase": {
    "type": "item"
  },
  "attributes": {
    "Hub": ["Nvidia", "Dataloop"],
    "Category": ["Application"],
    "Application Type": "GenAI",
    "Provider": "NVIDIA",
    "Developed By": "Dataloop"
  },
  "components": {
    "computeConfigs": [
      {
        "name": "biomedical-aiq-compute",
        "runtime": {
          "podType": "regular-xs",
          "concurrency": 10,
          "runnerImage": "gcr.io/viewo-g/piper/agent/runner/apps/nvidia-nim-blueprints:0.0.4",
          "autoscaler": {
            "type": "rabbitmq",
            "minReplicas": 0,
            "maxReplicas": 2
          }
        }
      }
    ],
    "pipelineNodes": [
      {
        "name": "nvidia-aiq-research-node",
        "invoke": {
          "type": "function",
          "namespace": "biomedical-aiq-compute.biomedical-aiq-module.execute_research"
        },
        "categories": ["NVIDIA Biomedical AI-Q"],
        "scope": "project"
      },
      {
        "name": "nvidia-aiq-reflect-node",
        "invoke": {
          "type": "function",
          "namespace": "biomedical-aiq-compute.biomedical-aiq-module.evaluate_reflection"
        },
        "categories": ["NVIDIA Biomedical AI-Q"],
        "scope": "project"
      },
      {
        "name": "nvidia-aiq-report-prep-node",
        "invoke": {
          "type": "function",
          "namespace": "biomedical-aiq-compute.biomedical-aiq-module.prepare_report"
        },
        "categories": ["NVIDIA Biomedical AI-Q"],
        "scope": "project"
      },
      {
        "name": "nvidia-aiq-output-node",
        "invoke": {
          "type": "function",
          "namespace": "biomedical-aiq-compute.biomedical-aiq-module.write_output"
        },
        "categories": ["NVIDIA Biomedical AI-Q"],
        "scope": "project"
      }
    ],
    "modules": [
      {
        "name": "biomedical-aiq-module",
        "computeConfig": "biomedical-aiq-compute",
        "entryPoint": "biomedical_aiq/biomedical_aiq_agent.py",
        "className": "BiomedicalAIQAgent",
        "integrations": [
          "dl-nvidia-api-key",
          "dl-tavily-api-key"
        ],
        "functions": [
          {
            "name": "execute_research",
            "input": [
              {
                "name": "item",
                "type": "Item"
              }
            ],
            "output": [
              {
                "name": "item",
                "type": "Item"
              }
            ],
            "displayName": "NVIDIA AI-Q Research",
            "displayIcon": "icon-dl-search"
          },
          {
            "name": "evaluate_reflection",
            "input": [
              {
                "name": "item",
                "type": "Item"
              }
            ],
            "output": [
              {
                "name": "item",
                "type": "Item",
                "actions": [
                  "research_more",
                  "finalize"
                ]
              }
            ],
            "displayName": "NVIDIA AI-Q Reflect",
            "displayIcon": "icon-dl-recycle"
          },
          {
            "name": "prepare_report",
            "input": [
              {
                "name": "item",
                "type": "Item"
              }
            ],
            "output": [
              {
                "name": "item",
                "type": "Item"
              }
            ],
            "displayName": "NVIDIA AI-Q Report Prep",
            "displayIcon": "icon-dl-edit"
          },
          {
            "name": "write_output",
            "input": [
              {
                "name": "item",
                "type": "Item"
              }
            ],
            "output": [
              {
                "name": "item",
                "type": "Item"
              }
            ],
            "displayName": "NVIDIA AI-Q Output",
            "displayIcon": "icon-dl-file-document"
          }
        ]
      }
    ]
  }
}
